<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PercentOfLimit__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  MaximumValue__c != 0,
  Value__c /  MaximumValue__c,
  0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The percent of the limit used in the org at time of snapshot</inlineHelpText>
    <label>Percent of Limit</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
